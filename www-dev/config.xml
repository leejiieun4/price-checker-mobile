<?xml version='1.0' encoding='utf-8'?>
<widget id="uk.co.mattgoldspink.makemecash" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>PriceCheckerMobile</name>
    <description>
        Make Me Cash App allows you to find which sites will buy your DVDs, CDs and Games for the best prices
    </description>
    <author email="makemecashapp@gmail.com" href="http://makemecashapp.launchrock.com">
        Matt Goldspink
    </author>
    <access origin="http://price-app-checker-eu.herokuapp.com" />
    <preference name="fullscreen" value="true" />
    <preference name="webviewbounce" value="false" />
    <preference name="UIWebViewBounce" value="false" />
    <preference name="show-splash-screen-spinner" value="false" />
    <preference name="detect-data-types" value="false" />
    <preference name="AutoHideSplashScreen" value="false" />
    <preference name="ShowSplashScreenSpinner" value="false" />
    <preference name="CFBundleDisplayName" value="Price Scan" />
    <content src="index.html" />
</widget>
